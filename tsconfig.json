{
  "include": [
    "node-service/**/*",
    "chatalog-cli/**/*",
    "report-page"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "sourceMap": true,
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "target": "ES6",
    "module": "CommonJS",
    "moduleResolution": "node",
    "lib": [
      "ES6"
    ],
    "esModuleInterop": true
  }
}